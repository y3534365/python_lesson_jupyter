    "## 创建一个数据集"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "metadata": {},
   "outputs": [],
   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "%matplotlib inline\n",
    "from sklearn.datasets.samples_generator import make_blobs"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "# X为样本特征，Y为样本簇类别， 共1000个样本，\n",
    "# 每个样本 2 个特征，共4个簇，\n",
    "# 簇中心在[-1,-1], [0,0],[1,1], [2,2]， \n",
    "# 簇标准差分别为[0.4, 0.2, 0.2,0.2]\n",
    "\n",
    "X, y = make_blobs(n_samples=1000,n_features=2,\n",
    "      centers= [[-1,-1], [0,0],[1,1], [2,2] ],\n",
    "          cluster_std=[0.4, 0.2, 0.2,0.2],random_state=666)"
   ]
  },
